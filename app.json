{
  "expo": {
    "name": "mmkv-widget-demo",
    "slug": "mmkv-widget-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "appleTeamId": "YOUR_TEAM_ID",
      "bundleIdentifier": "com.example.rnmmkv.widget.demo",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.example.rnmmkv.widget.demo"
        ]
      },
      "infoPlist": {
        "AppGroup": "group.com.example.rnmmkv.widget.demo"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "@bacons/apple-targets",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
